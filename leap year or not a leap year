#include<stdio.h>
int main()
{
    int a;
    scanf("%d",&a);
    if(a>1000)
    {
        if(a<10000)
        {
            if(a%100!=0)
            {
                if(a%4==0)
                {
                    printf("Leap year");
                }
                else if(a%400==0)
                {
                    printf("Leap year");
                }
            
                else
                {
                    printf("Not a Leap Year");
                }
            }
        }
    }
}
